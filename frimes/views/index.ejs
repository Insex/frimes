                    <img class="movieImg" v-bind:alt="film.name" align="middle" v-bind:src="film.imgDes">
                </div>

                <span class="film">
	           
                   <p class="name" v-url="'/player?<%-collection%>='+film.<%-link%>">{{film.name}}</p>
               
	
	           <div class="country" >
                   <span v-for="(country,index) in film.country">{{country}}
                        {{index==film.country.length-1?'':', '}}
                   </span>
            </div>

            <div class="zhanr">
                <span class="letter" v-on:click="setGanre(film.genres,index)" v-for="(ganre,index) in film.genres">
                        <span class="animGanre">{{ganre}}</span>{{index==film.genres.length-1?'':', '}}
                </span>
            </div>
            <div class="reiting">
                <span style="text-align:left">
                       <img src="img/star.svg">
		                      {{film.rating}}
                   </span>
            </div>

            </span>
            <span class="reitingYear ng-binding">
                {{film.year}}
            </span>

        </div>
        <div class="nothingFound" v-bind:style="{display:movies.length==0?'block':'none'}" >
