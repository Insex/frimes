            </div>
    </div>

    <div class="content" >

        <div class="back backGround">
            <input placeholder="<%-title%>" v-model="input" class="inp inpStyle">
        </div>

        <div class="divForFilms" v-cloak>
            <div class="films filmsMargin margin" v-for="film in movies">
                <div v-url="'/player?<%-collection%>='+film.<%-link%>" id="playButton" class="playButton notPlayButton">
                    <!--<div id="recommendation">
                    </div>
                    <svg id="button" viewBox="0 0 86 100" class="svg">
 				       <polygon  points="0,0 0,100 86,50"/>
